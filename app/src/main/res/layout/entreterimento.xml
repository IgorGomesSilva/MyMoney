<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/itemtela"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="igor.com.br.money.entidades.Main_inicio"
    android:background="@android:color/background_light">
    <ImageView
        android:layout_width="133dp"
        android:layout_height="31dp"
        android:src="@drawable/entre"
        android:layout_gravity="center"
        android:layout_weight="0.26" />
    <ListView

        android:id="@+id/listaEntre"
        android:layout_width="wrap_content"
        android:layout_height="308dp"
        android:layout_weight="0.99"
        android:background="@android:color/background_light"
        android:cacheColorHint="@android:color/background_light">

    </ListView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Valor R$:"
        android:textSize="20dp"
        android:fontFamily="serif"
        android:id="@+id/textoEntre"
        android:textStyle="normal|bold|italic"
        android:typeface="sans" />



</LinearLayout>
